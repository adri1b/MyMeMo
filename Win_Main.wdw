  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      // init vars
      SqlString, SqlCount, SqlFilter, SqlInclude, SqlExclude, SearchString, Created, Updated sont des chaines
      SqlQuery est une source de données
      i, j, RowID sont des entiers sur 8 octets
      
      // for pagination, we need to use variables...
      CurrentPage, RowsStart, RowsTotal, RowsResidual, PagesTotal, RowsOffset sont des entiers = (1, 0, 0, 0, 0, Config["RowsOffset"])
      
      // init table arrays
      TableSettings, SearchSettings sont des tableaux associatifs de Variants
      Tasks est un tableau de TâchesParallèles
      
      // init breaking feature
      BreakHead, BreakLib sont des Champs
      Parsed est une chaine
      
      // init structure
      Breaking_Data_Struct est une Structure
      	Ref est une chaîne
      	Cnt est un entier
      FIN
      Breaking_Data est un tableau de Breaking_Data_Struct
   -
     code : |1-
      
     type : 34
   -
     code : |1-
      // indent data
      Table..RetraitGauche = 10
      
      // footer
      Message(ChaîneConstruit("MyMeMo %1", ExeInfo(exeVersion)))
     type : 234
   -
     code : |1-
      
     type : 177
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      
     type : 2
  procedures :
   -
     name : QueryFilter
     procedure_id : 1354810140333580830
     type_code : 14
     code : |1-
      PROCEDURE privée QueryFilter()
      
      // reset vars
      POUR CHAQUE Key DE ["Filter", "Include", "Exclude"]
      	SI {"Sql" + Key, indVariable} <> "" ALORS
      		{"Sql" + Key, indVariable} = ""
      	FIN
      FIN
      
      SI (SearchSettings["Keywords"]..Existe = vrai) _et_ (SearchSettings["Keywords"] <> "") ALORS
      	// define specific vars, only for this feature!!
      	Include, Exclude sont des tableaux de chaînes
      	
      	Delimiter est un entier = 0
      	Delimited est une chaine
      	
      	SI ChaîneCommencePar(SearchSettings["Keywords"], ["+", "-", " "]) = 0 ALORS
      		TableauAjoute(Include, ExtraitChaîne(SearchSettings["Keywords"], rangPremier, ["+", "-", " "]))
      	FIN
      	SI Contient(SearchSettings["Keywords"], ["+", "-", " "]) = Vrai ALORS
      		POUR TOUTE CHAÎNE Substring, n DE SearchSettings["Keywords"] SÉPARÉE PAR ["+", "-", " "]
      			SI n <> 1 ALORS
      				Delimiter = (n - 1)
      				Delimited = SearchSettings["Keywords"][[Delimiter À (Taille(Substring) + Delimiter)]]
      				SELON Gauche(Delimited, 1)
      					CAS "+", " "
      						Parsed = Remplace(Delimited, ["+", " "], "")
      						SI Parsed <> "" ALORS
      							TableauAjoute(Include, Minuscule(Parsed))
      						FIN
      					CAS "-"
      						Parsed = Remplace(Delimited, "-", "")
      						SI Parsed <> "" ALORS
      							TableauAjoute(Exclude, Minuscule(Parsed))
      						FIN
      				FIN
      			FIN
      		FIN
      	FIN
      	
      	POUR CHAQUE Key DE ["Include", "Exclude"]
      		POUR CHAQUE Data DE {Key, indVariable}
      			pour chaque Column de ["Title", "Content", "Attachments"]
      				SI {"Sql" + Key, indVariable} <> "" ALORS
      					{"Sql" + Key, indVariable} += " OR"
      				FIN
      				
      				{"Sql" + Key, indVariable} += " " + Column + " LIKE '%" + SQLite.EscapeString(Data) + "%'"
      			FIN
      		FIN
      		
      		SI (Key = "Exclude") _et_ (SqlExclude <> "") ALORS
      			SqlExclude = (" NOT " + SqlExclude)
      		FIN
      	FIN
      FIN
      
      SI (SearchSettings["Categories"]..Existe = Vrai) _ET_ (SearchSettings["Categories"] <> "") ALORS
      	SqlInclude += ((SqlInclude <> "" ? "AND" SINON "") + " CatID ")
      	
      	selon contient(SearchSettings["Categories"], ",")
      		CAS vrai
      			SqlInclude += "IN (" + SearchSettings["Categories"] + ")"
      		cas faux
      			SqlInclude += "= " + SearchSettings["Categories"]
      	FIN
      FIN
      
      // filter dates values
      POUR CHAQUE Key DE ["Created", "Updated"]
      	SI ((SearchSettings[Key + "_From"]..Existe = Vrai) _ET_ (SearchSettings[Key + "_From"] <> "")) _OU_ ((SearchSettings[Key + "_To"]..Existe = Vrai) _ET_ (SearchSettings[Key + "_To"] <> "")) ALORS
      		SI (SearchSettings[Key + "_From"] <> "") _ET_ (SearchSettings[Key + "_To"] = "") ALORS
      			SqlInclude += ((SqlInclude <> "" ? "AND" SINON "") + " strftime('%Y-%m-%d', datetime(" + Key + ", 'unixepoch')) >= '" + DateVersChaîne(SearchSettings[Key + "_From"], "AAAA-MM-JJ") + "'")
      		SINON SI (SearchSettings[Key + "_From"] = "") _ET_ (SearchSettings[Key + "_To"] <> "") ALORS
      			SqlInclude += ((SqlInclude <> "" ? "AND" SINON "") + " strftime('%Y-%m-%d', datetime(" + Key + ", 'unixepoch')) <= '" + DateVersChaîne(SearchSettings[Key + "_To"], "AAAA-MM-JJ") + "'")
      		SINON SI (SearchSettings[Key + "_From"] <> "") _ET_ (SearchSettings[Key + "_To"] <> "") ALORS
      			SI SearchSettings[Key + "_From"] = SearchSettings[Key + "_To"] ALORS
      				SqlInclude += ((SqlInclude <> "" ? "AND" SINON "") + " strftime('%Y-%m-%d', datetime(" + Key + ", 'unixepoch')) = '" + DateVersChaîne(SearchSettings[Key + "_From"], "AAAA-MM-JJ") + "'")
      			SINON
      				SqlInclude += ((SqlInclude <> "" ? "AND" SINON "") + " strftime('%Y-%m-%d', datetime(" + Key + ", 'unixepoch')) BETWEEN '" + DateVersChaîne(SearchSettings[Key + "_From"], "AAAA-MM-JJ") + "' AND '" + DateVersChaîne(SearchSettings[Key + "_To"], "AAAA-MM-JJ") + "'")
      			FIN
      		FIN
      	FIN
      FIN
      
      // show memos with attached files?
      si SearchSettings["Attached"]..Existe = Vrai ALORS
      	SqlInclude += ((SqlInclude <> "" ? " AND" SINON "") + " Attached = " + SearchSettings["Attached"])
      FIN
      
      POUR CHAQUE Key DE ["Include", "Exclude"]
      	SI {"Sql" + Key, indVariable} <> "" ALORS
      		SqlFilter += ((Contient(SqlFilter, "WHERE") ? " AND" SINON " WHERE") + {"Sql" + Key, indVariable})
      	FIN
      FIN
      
      RENVOYER SqlFilter
     type : 458752
