    code_elements :
     type_code : 9
     p_codes :
      -
        code : |1-
         // disable display
         MoiMême..AffichageActif = Faux
         
         // remove all breaks
         RuptureSupprimeTout(MoiMême)
         
         // get position
         TableSettings["Position"] = TableSauvePositionEtSélection(MoiMême)
         
         // get search parameters
         QueryFilter
         
         si Col_Category..Visible = faux ALORS
         	Col_Category..Visible = vrai
         FIN
         
         // are there hidden fields/columns?
         SI Config["BreakDefault"] <> "None" ALORS
         	// hide/show columns
         	SELON Config["BreakDefault"]
         		CAS "Category"
         			Col_Category..Visible = Faux
         		AUTRE CAS
         			// small craftiness, if the case is not here!
         	FIN
         	
         	BreakHead <- RuptureAjoute("Col_" + Config["BreakDefault"], "Break_" + Config["BreakDefault"], ruptHaut, 22)
         	BreakHead..CouleurFond = HTMLVersRVB(Config["BreakColor"])
         	
         	BreakLib <- ChampCrée(BreakHead..NomComplet + ".Lib_Break_" + Config["BreakDefault"], typLibellé, BreakHead..X + 24, BreakHead..Y, 500, 20)
         	BreakLib..PoliceGras = Vrai
         	BreakLib..CadrageVertical = cvMilieu
         	
         	// count rows
         	SqlString = "SELECT ROWID, "
         	SELON Config["BreakDefault"]
         		CAS "Category"
         			SqlString += "CatID"
         		CAS "Created"
         			SqlString += "strftime('%Y%m%d', datetime(Created, 'unixepoch'))"
         		CAS "Updated"
         			SqlString += "strftime('%Y%m%d', datetime(Updated, 'unixepoch'))"
         	FIN
         	SqlString += " AS Ref"
         	SqlString += " FROM View_Memos"
         	SI SqlFilter <> "" ALORS
         		SqlString += SqlFilter
         	FIN
         	
         	SI HExécuteRequêteSQL(SqlQuery, DbSoftware, hRequêteSansCorrection, SqlString) = Faux ALORS
         		TitreSuivant("Erreur")
         		Erreur(HErreurInfo)
         		
         		Ferme
         	SINON
         		SI Breaking_Data..Occurrence >= 1 ALORS
         			TableauSupprimeTout(Breaking_Data)
         		FIN
         		
         		HLitPremier(SqlQuery, hSansRafraîchir)
         		
         		TANTQUE HEnDehors(SqlQuery) = Faux
         			i = TableauCherche(Breaking_Data, tcLinéaire, "Ref", SqlQuery.Ref)
         			SI i = -1 ALORS
         				i = TableauAjoute(Breaking_Data, [SqlQuery.Ref, 0])
         			FIN
         			
         			Breaking_Data[i].Cnt += 1
         			
         			HLitSuivant(SqlQuery)
         		FIN
         		
         		// free memory
         		HLibèreRequête(SqlQuery)
         	FIN
         FIN
         
         // init subquery
         SqlCount = "SELECT COUNT(ROWID)"
         SqlCount += " FROM View_Memos"
         SI SqlFilter <> "" ALORS
         	SqlCount += SqlFilter
         FIN
         
         // now show data in table!
         SqlString = "SELECT *, (" + SqlCount + ") AS Total"
         SqlString += " FROM View_Memos"
         SI SqlFilter <> "" ALORS
         	SqlString += SqlFilter
         FIN
         SqlString += " ORDER BY"
         SI Config["BreakDefault"] <> "None" ALORS
         	selon Config["BreakDefault"]
         		CAS "Category"
         			SqlString += " Category, "
         		cas "Updated"
         			SqlString += " Updated DESC, "
         		autre cas
         			// small craftiness, if the case is not here!
         	FIN
         FIN
         SqlString += " Created DESC"
         SqlString += (" LIMIT " + RowsOffset)
         si RowsStart > 0 ALORS
         	SqlString += (" OFFSET " + RowsStart)
         FIN
         
         SI HExécuteRequêteSQL(SqlQuery, DbSoftware, hRequêteSansCorrection, SqlString) = faux ALORS
         	TitreSuivant("Erreur")
         	Erreur(HErreurInfo)
         SINON
         	si MoiMême..Occurrence >= 1 ALORS
         		TableSupprimeTout(MoiMême)
         	FIN
         	
         	HLitPremier(SqlQuery, hSansRafraîchir)
         	TANTQUE HEnDehors(SqlQuery) = faux
         		SI (RowsTotal = 0) _OU_ (RowsTotal <> SqlQuery.Total) ALORS
         			RowsTotal = SqlQuery.Total
         		FIN
         		
         		i = TableAjouteLigne(MoiMême, SqlQuery.ROWID, SqlQuery.Attached, SqlQuery.Created, SqlQuery.Updated, "", SqlQuery.Category)
         		
         		Col_Created[i] = gauche(EpochVersDateHeure(Col_Created[i]), 8)
         		si Col_Updated[i] pas dans ("", 0) ALORS
         			Col_Updated[i] = Gauche(EpochVersDateHeure(Col_Updated[i]), 8)
         		FIN
         		
         		SI Config["BreakDefault"] <> "None" ALORS
         			BreakHead..Traitement[trtAffichageLigne] = BreakTitle
         			
         				PROCÉDURE INTERNE BreakTitle
         					
         					SELON Config["BreakDefault"]
         						cas "Category"
         							BreakLib[IndiceEnCours] = Col_Category
         							
         						CAS "Created"
         							Parsed = DateVersChaîne(Col_Created, Config["BreakDateTimeFormat"])
         							j = tableaucherche(Breaking_Data, tcLinéaire, "Ref", Col_Created)
         							si j <> -1 ALORS
         								BreakLib[IndiceEnCours] = ChaîneConstruit("Note%2 créée%2 le %1", Parsed, (Breaking_Data[j].Cnt >= 2 ? "s" sinon ""))
         							FIN
         							
         						CAS "Updated"
         							Updated = Col_Updated
         							si Updated dans ("", 0) alors
         								BreakLib[IndiceEnCours] = "Pas de mise à jour"
         							sinon
         								Parsed = DateHeureVersChaîne(Updated, Config["BreakDateTimeFormat"])
         								j = TableauCherche(Breaking_Data, tcLinéaire, "Ref", Col_Updated)
         								SI j <> -1 ALORS
         									BreakLib[IndiceEnCours] = ChaîneConstruit("Note%2 mise%2 à jour le %1", Parsed, (Breaking_Data[j].Cnt >= 2 ? "s" SINON ""))
         								FIN
         							FIN
         					FIN
         				FIN
         			fin
         		
         		Col_Container[i].Lib_Title = SqlQuery.Title
         		Col_Container[i].Img_Attached..Visible = Col_Attached[i]
         		
         		// dates
         		Parsed = ""
         		SI Config["BreakDefault"] <> "Created" ALORS
         			Parsed += ChaîneConstruit("Créée le %1", DateHeureVersChaîne(EpochVersDateHeure(SqlQuery.Created), "JJ/MM/AAAA à HH:mm"))
         		FIN
         		
         		SI Config["BreakDefault"] <> "Updated" ALORS
         			si Col_Updated[i] pas dans ("", 0) ALORS
         				si Parsed <> "" ALORS
         					Parsed += rc
         				FIN
         				
         				Parsed += ChaîneConstruit("Mise à jour le %1", DateHeureVersChaîne(EpochVersDateHeure(SqlQuery.Updated), "JJ/MM/AAAA à HH:mm"))
         			FIN
         		fin
         		
         		Col_Dates[i] = Parsed
         		
         		HLitSuivant(SqlQuery)
         	FIN
         	
         	HLibèreRequête(SqlQuery)
         FIN
         
         // retrieve the position
         TableRestaurePositionEtSélection(MoiMême, TableSettings["Position"])
         
         // calculate total pages
         RowsResidual = modulo(RowsTotal, RowsOffset)
         SELON RowsResidual
         	CAS 0
         		PagesTotal = (RowsTotal / RowsOffset)
         	AUTRE CAS
         		PagesTotal = (((RowsTotal - RowsResidual) / RowsOffset) + 1)
         FIN
         
         SELON PagesTotal
         	CAS 0, 1
         		POUR CHAQUE Button DE ["Start", "Previous", "Next", "End"]
         			{"Btn_Page_" + Button, indChamp}..Visible = Faux
         		FIN
         		Lib_Total_Pages..Visible = Faux
         		
         	AUTRE CAS
         		POUR CHAQUE Button DE ["Start", "Previous", "Next", "End"]
         			SI {"Btn_Page_" + Button, indChamp}..Visible = Faux ALORS
         				{"Btn_Page_" + Button, indChamp}..Visible = Vrai
         			FIN
         		FIN
         		SI Lib_Total_Pages..Visible = Faux ALORS
         			Lib_Total_Pages..Visible = Vrai
         		FIN
         		
         		Lib_Total_Pages = "Page %1 de %2"
         		SELON CurrentPage
         			CAS 1
         				POUR CHAQUE Button DE ["Start", "Previous"]
         					SI {"Btn_Page_" + Button, indChamp}..Etat = Actif ALORS
         						{"Btn_Page_" + Button, indChamp}..Etat = Grisé
         					FIN
         				FIN
         				POUR CHAQUE Button DE ["Next", "End"]
         					SI {"Btn_Page_" + Button, indChamp}..Etat = Grisé ALORS
         						{"Btn_Page_" + Button, indChamp}..Etat = Actif
         					FIN
         				FIN
         				
         				Lib_Total_Pages = ChaîneConstruit(Lib_Total_Pages, 1, PagesTotal)
         				
         			CAS PagesTotal
         				POUR CHAQUE Button DE ["Start", "Previous"]
         					SI {"Btn_Page_" + Button, indChamp}..Etat = Grisé ALORS
         						{"Btn_Page_" + Button, indChamp}..Etat = Actif
         					FIN
         				FIN
         				POUR CHAQUE Button DE ["Next", "End"]
         					SI {"Btn_Page_" + Button, indChamp}..Etat = Actif ALORS
         						{"Btn_Page_" + Button, indChamp}..Etat = Grisé
         					FIN
         				FIN
         				
         				Lib_Total_Pages = ChaîneConstruit(Lib_Total_Pages, PagesTotal, PagesTotal)
         				
         			AUTRE CAS
         				POUR CHAQUE Button DE ["Start", "Previous", "Next", "End"]
         					SI {"Btn_Page_" + Button, indChamp}..Etat = Grisé ALORS
         						{"Btn_Page_" + Button, indChamp}..Etat = Actif
         					FIN
         				FIN
         				
         				Lib_Total_Pages = ChaîneConstruit(Lib_Total_Pages, CurrentPage, PagesTotal)
         		FIN
         FIN
         
         // enable display
         MoiMême..AffichageActif = Vrai
