     p_codes :
      -
        code : |1-
         SqlString = "SELECT ROWID AS Data_ID, Title AS Data_Name"
         SqlString += " FROM Categories"
         
         SI SearchString <> "" ALORS
         	POUR CHAQUE Key DE ["Include", "Exclude"]
         		SI {"Sql" + Key, indVariable} <> "" ALORS
         			{"Sql" + Key, indVariable} = ""
         		FIN
         	FIN
         	
         	// define specific vars, only for this feature!!
         	Include, Exclude sont des tableaux de chaînes
         	
         	Delimiter est un entier = 0
         	Delimited, Parsed sont des chaînes
         	
         	SI ChaîneCommencePar(SearchString, ["+", "-", " "]) = 0 ALORS
         		TableauAjoute(Include, ExtraitChaîne(SearchString, rangPremier, ["+", "-", " "]))
         	FIN
         	SI Contient(SearchString, ["+", "-", " "]) = Vrai ALORS
         		POUR TOUTE CHAÎNE Substring, n DE SearchString SÉPARÉE PAR ["+", "-", " "]
         			SI n <> 1 ALORS
         				Delimiter = (n - 1)
         				Delimited = SearchString[[Delimiter À (Taille(Substring) + Delimiter)]]
         				SELON Gauche(Delimited, 1)
         					CAS "+", " "
         						Parsed = Remplace(Delimited, ["+", " "], "")
         						SI Parsed <> "" ALORS
         							TableauAjoute(Include, Minuscule(Parsed))
         						FIN
         					CAS "-"
         						Parsed = Remplace(Delimited, "-", "")
         						SI Parsed <> "" ALORS
         							TableauAjoute(Exclude, Minuscule(Parsed))
         						FIN
         				FIN
         			FIN
         		FIN
         	FIN
         	
         	SqlGen est une chaîne
         	POUR CHAQUE Key DE ["Include", "Exclude"]
         		POUR CHAQUE Data DE {Key, indVariable}
         			SI {"Sql" + Key, indVariable} <> "" ALORS
         				{"Sql" + Key, indVariable} += " OR "
         			FIN
         			
         			{"Sql" + Key, indVariable} += ("LOWER(Title) LIKE '%" + SQLite.EscapeString(Data) + "%'")
         		FIN
         		
         		SI {"Sql" + Key, indVariable} <> "" ALORS
         			SqlGen = {"Sql" + Key, indVariable}
         			
         			{"Sql" + Key, indVariable} = (" " + (Contient(SqlString, "WHERE") ? "AND" SINON "WHERE") + " ROWID")
         			SI Key = "Exclude" ALORS
         				{"Sql" + Key, indVariable} += " NOT"
         			FIN
         			{"Sql" + Key, indVariable} += (" IN (SELECT ROWID FROM Categories WHERE " + SqlGen + ")")
         		FIN
         	FIN
         	
         	POUR CHAQUE Key DE ["Include", "Exclude"]
         		SI {"Sql" + Key, indVariable} <> "" ALORS
         			SqlString += {"Sql" + Key, indVariable}
         		FIN
         	FIN
         FIN
         
         SqlString += " ORDER BY Title"
         
         // fill table with database data
         SI HExécuteRequêteSQL(SqlQuery, DbSoftware, hRequêteSansCorrection, SqlString) = faux ALORS
         	TitreSuivant("Erreur")
         	Erreur(HErreurInfo)
         SINON
         	// clean table
         	si MoiMême..Occurrence >= 1 ALORS
         		TableSupprimeTout(MoiMême)
         	FIN
         	
         	SI HNbEnr(SqlQuery) >= 1 ALORS
         		HLitPremier(SqlQuery, hSansRafraîchir)
         		TANTQUE HEnDehors(SqlQuery) = faux
         			TableAjouteLigne(MoiMême, SqlQuery.Data_ID, Faux, SqlQuery.Data_Name)
         			
         			HLitSuivant(SqlQuery)
         		FIN
         	FIN
         	
         	// free memory
         	HLibèreRequête(SqlQuery)
         FIN
        type : 14
