       name : Opt_Select_Only
       identifier : 0x299aad27362cf175
       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            // add ids in array!
            POUR CHAQUE LIGNE DE Table
            	SI Col_Check = Vrai ALORS
            		TableauAjoute(Selection, Col_ID)
            	FIN
            FIN
            
            // let's go!!
            SELON Selection..Occurrence
            	CAS 0
            		MoiMême..cochée = faux
            		
            	AUTRE CAS
            		// purge table!
            		TableSupprimeTout(Table)
            		
            		// fill it!
            		SELON MoiMême..cochée
            			CAS Vrai
            				POUR CHAQUE Column DE SqlData
            					TableAjouteLigne(Table, Column.ID, Faux, Column.Name)
            				FIN
            				POUR CHAQUE ID DE Selection
            					i = TableCherche(Col_ID, ID)
            					SI i <> -1 ALORS
            						Col_Check[i] = Vrai
            					FIN
            				FIN
            				
            				// un-check option
            				MoiMême..Cochée = Faux
            				
            			CAS Faux
            				POUR CHAQUE ID DE Selection
            					i = TableauCherche(SqlData, tcLinéaire, "ID", ID)
            					SI i <> -1 ALORS
            						TableAjouteLigne(Table, SqlData[i].ID, vrai, SqlData[i].Name)
            					FIN
            				FIN
            				
            				// check option
            				MoiMême..Cochée = Vrai
            		FIN
            FIN
            
            // clean array!
            TableauSupprimeTout(Selection)
