  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      // init vars, etc.
      SqlQuery est une source de données
      SqlString, SqlInclude, SqlExclude, SqlColumns, SearchString sont des chaines
      i, Cnt sont des entiers
      
      Selection est un tableau d'entiers
      
      // init structures
      SqlData_Structure est une Structure
      	ID est un entier
      	Name est une chaîne
      FIN
      SqlData est un tableau de SqlData_Structure
      
      // get ids
      Checked_Int est un tableau d'entiers
      SI Win_Search.SearchSettings["Categories"]..Existe = Vrai ALORS
      	ChaîneVersTableau(Win_Search.SearchSettings["Categories"], Checked_Int, ",")
      	Cnt = Checked_Int..Occurrence
      FIN
   -
     code : |1-
      
     type : 34
   -
     code : |1-
      // tick columns
      SI Cnt >= 1 ALORS
      	POUR CHAQUE Indexed de Checked_Int
      		i = TableCherche(Col_ID, Indexed, rechercheIdentique)
      		SI i <> -1 ALORS
      			Col_Check[i] = Vrai
      		FIN
      	FIN
      FIN
      
      // indent data
      Table.Col_Name..RetraitGauche = 10
