     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1-
         // reset var
         Cnt = 0
         	
         // clear content
         TableauSupprime(Win_Search.SearchSettings, "Categories")
         SI Win_Search.SearchSettings["Categories_Details"]..Existe = Vrai ALORS
         	TableauSupprime(Win_Search.SearchSettings, "Categories_Details")
         FIN
         
         // add selected ids in a array and relative names in a string
         POUR CHAQUE LIGNE DE Table
         	SI Col_Check = Vrai ALORS
         		SI Win_Search.SearchSettings["Categories"] <> "" ALORS
         			Win_Search.SearchSettings["Categories"] += ","
         		FIN
         		SI Win_Search.SearchSettings["Categories_Details"] <> "" ALORS
         			Win_Search.SearchSettings["Categories_Details"] += ", "
         		FIN
         		
         		Win_Search.SearchSettings["Categories"] += Col_ID
         		Win_Search.SearchSettings["Categories_Details"] += Col_Name
         		
         		Cnt++
         	FIN
         FIN
         
         // purge dropdown menu
         ListeSupprimeTout(Win_Search.List_Categories)
         
         // add default text if nothing has been selected
         SI Cnt = 0 ALORS
         	Win_Search.SearchSettings["Categories_Details"] = "Sélectionnez une catégorie"
         FIN
         
         // add this line in it
         ListeAjoute(Win_Search.List_Categories, Win_Search.SearchSettings["Categories_Details"])
         
         // select the first line
         ListeSelectPlus(Win_Search.List_Categories, 1)
         
         // close popup
         ferme
